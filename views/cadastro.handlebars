        {{!-- Tabela clientes --}}
        <div class="margem">
            <h1><a name="cadCL"></a>Cadastro de cliente</h1>
            <br><br>
            <form action="clie/insertclie" method="post">
                <div class="form-group">
                    <label for="exampleInputEmail1">Nome</label>
                    <input type="text" class="form-control" id="exampleInputNomecliente" name="nome_cliente"
                        aria-describedby="emailHelp" placeholder="Digite o seu nome">
                </div>

                <div class="form-group">
                    <label for="exampleInputCpf1">CPF</label>
                    <input type="text" class="form-control" id="exampleInputCpf" name="cpf" aria-describedby="emailHelp"
                        placeholder="Digite o seu CPF">
                </div>

                <div class="form-group">
                    <label for="exampleInputEmail1">Endereço de email</label>
                    <input class="form-control" id="exampleInputEmail1" name="email" placeholder="Digite seu email">
                </div>

                <div class="form-group">
                    <label for="exampleInputEmail1">Telefone</label>
                    <input type="number" class="form-control" id="exampleInputTelefone" name="telefone"
                        placeholder="Digite um número para contato">
                </div>

                <div class="form-group">
                    <label for="exampleInputAdress1">Endereço</label>
                    <input type="text" class="form-control" id="" name="endereco_cliente"
                        placeholder="Digite seu endereço">
                </div>

                <div class="form-group">
                    <label for="exampleInputEmprestimo1">Empréstimo</label>
                    <input type="number" class="form-control" id="" name="id_do_emprestimo"
                        placeholder="Digite o ID do emprestimo">
                </div>

                <div class="form-group">
                    <label for="exampleInputCartao1">Número do cartão</label>
                    <input type="number" class="form-control" id="" name="numero_do_cartao"
                        placeholder="Digite o número do cartao">
                </div>

                <div class="form-group">
                    <label for="exampleInputAgency">Agência</label>
                    <input type="number" class="form-control" id="" name="id_da_agencia"
                        placeholder="Digite o ID da agência">
                </div>

                <div class="form-group">
                    <button type="submit" class="btn btn-warning">Cadastrar</button>
                </div>

            </form>
